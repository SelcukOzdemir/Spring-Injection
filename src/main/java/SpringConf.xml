<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

 
   <bean id="employee" class="com.sso.injection.setter.Employee">
   		<property name="firstname" value="AAA"></property>
   		<property name="surname" value="BBB"></property>
   		<property name="salary" value="10000"></property>
   </bean>
 
	<bean id="car" class="com.sso.injection.constructor.Car">
		<constructor-arg type="String" value="Audi"></constructor-arg>
		<constructor-arg type="int" value="50000"></constructor-arg>
	</bean>
 
</beans>